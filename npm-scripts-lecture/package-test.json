{
  "version": "0.6.0",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "deploy:promote": "gulp clean:dist",
    "apply:version": "node applyVersion",
    "start": "node build/app",
    "tsc": "tsc",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "watch-tsc": "tsc --watch",
    "start-js": "react-scripts start",
    "dev-start": "npm run build-css && tsc && concurrently \"npm run watch-css\" \"npm run watch-tsc\" \"npm run start-js\"",
    "test": "react-scripts test --env=jsdom",
    "clean": "concurrently \"npm run clean:server:js\" \"npm run clean:server:map\" \"npm run clean:src:js\" \"npm run clean:src:map\" \"npm run clean:css\"",
    "clean:server:js": "rimraf server/**/*.js",
    "clean:server:map": "rimraf server/**/*.map",
    "clean:src:js": "rimraf src/**/*.js",
    "clean:src:map": "rimraf src/**/*.map",
    "clean:css": "rimraf src/**/*.css",
    "clean:build:map": "rimraf build/**/*.map",
    "aura-check": "aura",
    "prebuild": "rimraf build/**/*",
    "build": "npm run build-css && npm run tsc && react-scripts build && npm run build:workers && npm run build:server",
    "build:workers": "webpack --config config/webpack.workers.js --progress",
    "build:server": "webpack --config config/webpack.server.js --progress",
    "clean:dist": "rm -r ./dist",
    "build:2": "mkdir "
  },
  "dependencies": {
    "ajv": "5.2.1",
    "aws-sdk": "2.62.0",
    "better-queue": "3.8.5",
    "body-parser": "1.17.1",
    "compression": "1.6.2",
    "cors": "2.8.3",
    "elasticsearch": "13.1.0",
    "express": "4.15.2",
    "http-aws-es": "^1.1.3",
    "load-balancer": "1.0.6",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "queue": "4.2.1",
    "request": "2.81.0",
    "request-promise": "4.2.0",
    "rimraf": "2.6.1",
    "semver": "5.3.0",
    "socket.io": "1.7.3",
    "wk-aura-cli": "0.2.1",
    "wk-log": "1.1.3-ts23.3",
    "wk-vars": "0.8.1"
  },
  "devDependencies": {
    "classnames": "2.2.5",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.0",
    "environment-variables-webpack-plugin": "1.1.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "immutable": "3.8.1",
    "moment-duration-format": "1.3.0",
    "node-sass": "4.5.3",
    "postcss-loader": "1.3.3",
    "prop-types": "15.5.8",
    "raw-loader": "0.5.1",
    "react": "15.6.0",
    "react-dom": "15.6.0",
    "react-modal": "1.9.4",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-router-link-target": "^0.13.43",
    "react-scripts": "1.0.7",
    "redux": "3.6.0",
    "redux-logger": "2.10.2",
    "redux-thunk": "2.2.0",
    "sass-loader": "6.0.3",
    "script-ext-html-webpack-plugin": "1.8.1",
    "socket.io-client": "1.7.3",
    "source-map-loader": "0.2.1",
    "style-loader": "0.16.1",
    "ts-loader": "2.0.3",
    "typescript": "2.3.4",
    "webpack": "2.6.1",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": "8.1.0",
    "npm": "5.0.3"
  }
}
